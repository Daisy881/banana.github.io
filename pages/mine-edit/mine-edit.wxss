.container_mine-edit {
  position: fixed;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  display: flex;
  flex-direction: column;
  background-color: #f6f6f6;
}
.top-box {
  background: white;
}
.mineedit-box {
  padding: 20rpx 20rpx;
  border-bottom: 1px solid #f6f6f6;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.mineedit-box image {
  width: 30rpx;
  height: 30rpx;
  margin-top: 10rpx;
}
.exitLog {
  color: red;
  background: white;
  margin-top: 20rpx;
  padding: 20rpx 20rpx;
  border-bottom: 1px solid #f6f6f6;
  display: flex;
  justify-content: center;
}